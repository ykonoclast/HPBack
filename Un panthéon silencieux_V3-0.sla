        <PAGEOBJECT XPOS="156.69354327126" YPOS="27458.2889208184" OwnPage="61" ItemID="1133202452" PTYPE="4" WIDTH="510.2418898" HEIGHT="36.3482445359696" FRTYPE="0" CLIPEDIT="1" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L510.242 0 L510.242 36.3482 L0 36.3482 L0 0 Z" copath="M0 0 L510.242 0 L510.242 36.3482 L0 36.3482 L0 0 Z" gXpos="0" gYpos="0" gWidth="0" gHeight="0" PSTYLE="Intro partie" LAYER="1" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle PARENT="Intro partie"/>
